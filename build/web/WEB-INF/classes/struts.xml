<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.custom.i18n.resources" value="ApplicationResources" />
   <constant name="struts.devMode" value="true" />
    <constant name="struts.action.excludePattern" value="/image/*"/>
   
    <package name="default" extends="struts-default" >
      <result-types>
         <result-type name="tiles" 
         class="org.apache.struts2.views.tiles.TilesResult" />
      </result-types>
<action name="login" method="login" class="com.struts.MenuAction">
         <result name="login" type="tiles">login</result>
         
      </action>
      <action name="ser" method="ser" class="com.struts.MenuAction">
         <result name="ser" type="tiles">ser</result>
       
      </action>
      <action name="ser1" method="ser1" class="com.struts.MenuAction">
         <result name="ser1" type="tiles">ser1</result>
       
      </action>
       <action name="ser2" method="ser2" class="com.struts.MenuAction">
         <result name="ser2" type="tiles">ser2</result>
       
      </action>
       <action name="ser3" method="ser3" class="com.struts.MenuAction">
         <result name="ser3" type="tiles">ser3</result>
       
      </action>
       <action name="ser4" method="ser4" class="com.struts.MenuAction">
         <result name="ser4" type="tiles">ser4</result>
       
      </action>
       <action name="ser5" method="ser5" class="com.struts.MenuAction">
         <result name="ser5" type="tiles">ser5</result>
       
      </action>
      <action name="login1" method="login1" class="com.struts.LoginAction">
         <result name="success" type="tiles">login1</result>
         <result name="error" type="tiles">login</result>
      </action>
     
       <action name="rej" method="rej" class="com.struts.MenuAction">  
         <result name="rej" type="tiles">rej</result>
      </action>
      <action name="con" method="con" class="com.struts.MenuAction">  
         <result name="con" type="tiles">con</result>
      </action>
      <action name="ab" method="ab" class="com.struts.MenuAction">  
         <result name="ab" type="tiles">ab</result>
      </action>
      <action name="rej1" method="rej1" class="com.struts.RejAction">  
         <result name="succ" type="tiles">rej1</result>
         <result name="err" type="tiles">rej</result>
      </action>
      <action name="up" method="up" class="com.struts.MenuAction">  
         <result name="up" type="tiles">up</result>
      </action>
      <action name="upp" method="upp" class="com.struts.UpdateAction">  
         <result name="upp" type="tiles">upp</result>
         <result name="err" type="tiles">up</result>
      </action>
         <action name="upd" method="upd" class="com.struts.UpdateAction">  
         <result name="succ" type="tiles">upd</result>
         <result name="err" type="tiles">update1</result>
      </action>
        <action name="editImg" method="editImg" class="com.struts.editimg">  
         <result name="editImg" type="tiles">editImg</result>
         
      </action>
      <action name="upImg" method="upImg" class="com.struts.editimg">  
         <result name="upImg" type="tiles">upImg</result>
         
      </action>
      <action name="out" method="out" class="com.struts.LogoutAction">
         <result name="out" type="tiles">out</result>
       
      </action>
   </package>
</struts>